#include<iostream>
//输入一组数，输出最大值max和最小值min以及这组数的和
using namespace std;
int main()
{
	int m = 0; int max = m; int min = m;
	int i = 0; int n;//n表示数字个数
	cin >> n;
	//连续输入数字
	for (i = 0; i < n; i++)
	{
		cin >> m;
		if (i == 0)
		{
			max = m; min = m;
		}
		if (m > max)
		{
			max = m;
		}
		if (m < min)
		{
			min = m;
		}
	}

	cout << min << " " << max;


	return 0;
}
