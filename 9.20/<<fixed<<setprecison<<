#include<iostream>
#include<iomanip>
using namespace std;
int main()
{
    int a = 0;int b = 0;
    int d = 0;
    cin>>a>>b;
    double f = (double)a / b;
  	d = static_cast<int>(f);
    int r = 0;
    r = a % b;
    cout<<d<<" "<<r<<" "<<fixed<<setprecision(5)<<f<<endl;
    system("pause");
    return 0;
}
