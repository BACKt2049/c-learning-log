#include<iostream>
using namespace std;
int main()
{
	int a = 0; int b = 0; int c = 0; int d = 0;
	int n;
	cin >>n;
	if (n % 2 == 0 && (n > 4 && n <= 12)) { a = 1; }
	if (n % 2 == 0 || (n > 4 && n <= 12)) { b = 1; }
	if (!(n % 2 == 0 && (n > 4 && n <= 12))) { c = 1; }
	if (!(n % 2 == 0 || (n > 4 && n <= 12))) { d = 1; }
	cout << a << " " << b << " " << c << " " << d;
	return 0;
}
