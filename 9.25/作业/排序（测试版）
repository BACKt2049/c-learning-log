#include<iostream>
using namespace std;
int main()
{
	int a; int b; int c; int temp = 0;
	cin >> a; cin >> b; cin >> c;
	if (a > b)
	{
		temp = b;
		b = a;
		a = temp;
	}
	if (b > c) 
	{ temp = c;
	c = b; 
	b = temp; 
	}
	if (a > b) {
		temp = b;
		b = a; 
		a = temp; }
	cout << a << " " << b << " " << c;

	return 0;
}
